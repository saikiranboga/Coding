#include <algorithm>
#include <fstream>
#include <iostream>

#define MAX 1024
#define mp make_pair
#define f first
#define s second

using namespace std;

int n, m;
int up[MAX][MAX], sol[MAX][MAX];
int lungSt[MAX], lungDr[MAX];

int main()
{
    //ifstream cin("standard.in");
    //ofstream cout("standard.out");

    cin >> n >> m;

    for (int i = 1; i <= n; i++)
        for (int j = 1; j <= m; j++)
        {
            int a;
            cin >> a;

            if (a)
                up[i][j] = 0;
            else up[i][j] = up[i - 1][j] + 1;
        }

    for (int i = 1; i <= n; i++)
    {
        int msStv = 1;
        pair <int, int> stiva[MAX]; // In sus, pozitia
        for (int j = 0; j <= m; j++)
            stiva[j] = mp(0, 0);

        for (int j = 1; j <= m; j++)
        {
            stiva[++msStv] = mp(up[i][j], j);

            for (; stiva[msStv].f <= stiva[msStv - 1].f && msStv > 1; msStv--)
            {
                stiva[msStv - 1].f = stiva[msStv].f;

                stiva[msStv] = mp(0, 0);
            }

            lungSt[j] = j - stiva[msStv].s + 1;
        }

        msStv = 1;
        for (int j = 1; j <= m; j++)
            stiva[j] = mp(0, 0);
        stiva[1] = mp(0, m + 1);

        for (int j = m; j; j--)
        {
            stiva[++msStv] = mp(up[i][j], j);

            for (; stiva[msStv].f <= stiva[msStv - 1].f && msStv > 1; msStv--)
            {
                stiva[msStv - 1].f = stiva[msStv].f;

                stiva[msStv] = mp(0, 0);
            }

            lungDr[j] = stiva[msStv].s - j + 1;
        }

        int mas[MAX], l[MAX];
        for (int j = 0; j <= i; j++)
            mas[j] = 0, l[j] = 0;

        for (int j = 1; j <= m; j++)
            l[up[i][j]] = max(l[up[i][j]], lungDr[j] + lungSt[j] - 1);

        for (int j = i; j; j--)
        {
            l[j] = max(l[j + 1], l[j]);

            mas[j] = l[j] * j;
        }

        for (int j = 1; j <= i; j++)
            sol[i - j + 1][i] = max(max(sol[i - j + 1 + 1][i], sol[i - j + 1][i - 1]), mas[j]);
    }

    int q;
    for (cin >> q; q; q--)
    {
        int i, j;
        cin >> i >> j;
        cout << sol[i][j] << '\n';
    }

    return 0;
}
