        if (j==i)
        {
            int k=0;
            while (k<n && v[k].size()<=s) k++;
            while (i<=k)
            {
                BIT[i]--;
                i+=i&(-i);
            }
        }
    }
}*/

void sol()
{
    for (int i=1; i<=m; i++)
    {
        if (commands[i]==0)
        {
            int max=0;
            for (int i=1; i<=n; i++)
                if (max<v[i].size()) max=v[i].size();
                printf("%d\n", max);
        }
        else
        {
            int c=commands[i]+1;
            for (int i=2; i<=c; i++)
            {
                v[i].clear();
                r[i]=false;
            }
            for (int i=2; i<=n; i++)
            {
                for (int j=i-1; j>0; j--)
                if (!r[j])
                    if(a[j].depth<a[i].depth)
                    {
                        r[j]=true;
                        v[i].push_back(j);
                    }
            }
        }
    }
}

int main()
{
    Init();
    sol();
    return 0;
}
