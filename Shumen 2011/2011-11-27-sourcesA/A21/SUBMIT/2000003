#include <iostream>
#include <cstdio>
#include <cmath>
#include <cstring>
using namespace std;

bool a[1024][1024];
int b[1024];
int m, n, r1, r2;
int l;

int solve()
{
    int br=1, maxx=0;
    for(int i=r1; i<=r2; i++)
    {
        for(int k=1; k<=m; k++)
        {
            if(a[i][k]==0)b[k]++;
            else b[k]=0;
        }

        l=1;
        while(l<=m)
        {
            while(b[l]==b[l+1] && l+1<=m)
            {
                br++;
                l++;
            }
            if(br*b[l]>maxx)maxx=br*b[l];
            l++;
            br=1;
        }
    }

    memset(b, 0, sizeof b);
    return maxx;
}

void read()
{
    int p;
cin>>n>>m;
for(int i=1; i<=n; i++)
{
    for(int k=1; k<=m; k++)
    scanf("%d", &a[i][k]);
}

cin>>p;
for(int i=0; i<p; i++)
{
    scanf("%d%d", &r1, &r2 );
    cout<<solve()<<endl;
}

}

int main()
{
    read();
return 0;
}
