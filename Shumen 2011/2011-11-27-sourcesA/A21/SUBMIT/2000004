#include <iostream>
#include <cstdio>
#include <cmath>
#include <cstring>
using namespace std;

bool a[1024][1024];
int b[1024][1024];
int b2[1024][1024];
int m, n, r1, r2;
int l;

void calc()
{

for(int i=1; i<=n; i++)
for(int k=1; k<=m; k++)
if(a[i][k]==0) b[i][k]=b[i][k-1]+1;
else b[i][k]=0;



for(int i=1; i<=n; i++)
for(int k=m; k>0; k--)
if(a[i][k]==0) b2[i][k]=b2[i][k+1]+1;
else b2[i][k]=0;

}

int solve()
{
    int br=1, maxx=0;
    for(int i=1; i<=m; i++)
    {
        l=r1;
        while(l<=r2)
        {
            while(b[l][i]==b[l+1][i] && l+1<=r2)
            {
                br++;
                l++;
            }
            if(br*b[l][i]>maxx)maxx=br*b[l][i];
            l++;
            br=1;
        }

        l=r1;
        while(l<=r2)
        {
            while(b2[l][i]==b2[l+1][i] && l+1<=r2)
            {
                br++;
                l++;
            }
            if(br*b2[l][i]>maxx)maxx=br*b2[l][i];
            l++;
            br=1;
        }
    }

    memset(b, 0, sizeof b);
    return maxx;
}

void read()
{
    int p;
cin>>n>>m;
for(int i=1; i<=n; i++)
{
    for(int k=1; k<=m; k++)
    scanf("%d", &a[i][k]);
}

calc();


cin>>p;
for(int i=0; i<p; i++)
{
    scanf("%d%d", &r1, &r2 );
    cout<<solve()<<endl;
}

}

int main()
{
    read();
return 0;
}
