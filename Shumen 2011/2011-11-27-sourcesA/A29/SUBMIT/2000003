#include <cstdio>
#include <vector>
#include <algorithm>
using namespace std;

const int N = 1000010;

int n, m;
int niz[N], sol[N];

vector <int> V;

int main( void ) {
    scanf( "%d %d", &n, &m );
    for( int i = 0; i < n; ++i ) { scanf( "%d", &niz[i] ); niz[i] = -niz[i]; } 
    
    for( int i = 0; i < m; ++i ) {
         int a;
         scanf( "%d", &a );
         
         if( a ) { swap( niz[a-1], niz[a] ); continue; }   
         
         V.clear();
         int mx = 0;
         for( int j = 0; j < n; ++j ) sol[j] = 0;
         for( int j = n-1; j+1; --j ) {
              while( V.size() && niz[V.back()] >= niz[j] ) V.pop_back();
              if( V.size() ) ++sol[V.back()];
              V.push_back( j );
         }
         for( int j = 0; j < n; ++j ) mx = max( mx, sol[j] ); 
         printf( "%d\n", mx );
    }
    
    return 0;
}
