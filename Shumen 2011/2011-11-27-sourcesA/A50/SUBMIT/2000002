program shablon;

{$MODE OBJFPC}{$H+}
{$COperators ON}

Uses SysUtils;

const nmax = 1000;
      imax = 100;
      tmax = 10;

type tpoint = record
         x, y : extended;
     end;
     tvector = tpoint;

var n, k, i, j, t : longint;
    m : tpoint;
    dog, sh : array[1 .. nmax] of tpoint;
    sumold, sum : extended;
    vect : tvector;

operator + (a, b : tvector) : tvector;
begin
    result.x := a.x + b.x;
    result.y := a.y + b.y;
end;

operator - (a, b : tvector) : tvector;
begin
    result.x := a.x - b.x;
    result.y := a.y - b.y;
end;

procedure pinit;
begin
    assign(input, 'input.txt');
    reset(input);
    assign(output, 'output.txt');
    rewrite(output);
 end;

procedure pout;
begin
    close(input);
    close(output);
end;

function ro(a, b : tpoint) : extended;
begin
    result := sqrt(sqr(b.y - a.y) + sqr(b.x - a.x));
end;

function getsum : extended;
var i, j : longint;
    sum, sc, d : extended;
begin
    sum := 0;
    for i := 1 to n do begin
        sc := maxint;
        for j := 1 to k do begin
            d := ro(sh[i], dog[j]);
            if d < sc then begin
                sc := d;
            end;
        end;
        sum += sc;
    end;
   result := sum;
end;

function getvect : tvector;
begin
    result.x := random(trunc(m.x)) + random(1000001)/1000000;
    result.y := random(trunc(m.y)) + random(1000001)/1000000;
end;

begin
   // pinit;
    randomize;
    read(n, k);
    m.x := 0;
    m.y := 0;
    for i := 1 to n do begin
        read(sh[i].x, sh[i].y);
        if sh[i].x > m.x then begin
            m.x := sh[i].x;
        end;
        if sh[i].y > m.y then begin
            m.y := sh[i].y;
        end;
    end;

    for i := 1 to k do begin
        dog[i].x := random(trunc(m.x) + 1);
        dog[i].y := random(trunc(m.y) + 1);
    end;
    sum := getsum;
    for i := 1 to imax do begin
        j := random(k) + 1;
        for t := 1 to tmax do begin
            vect := getvect;
            dog[j] := dog[j] + vect;
            sumold := sum;
            sum := getsum;
            if sum > sumold then begin
                sum := sumold;
                dog[j] := dog[j] - vect;
            end;
        end;
    end;
    for i := 1 to k do begin
        writeln(dog[i].x :0 :6, ' ', dog[i].y :0 :6);
    end;
   // pout;
end.
