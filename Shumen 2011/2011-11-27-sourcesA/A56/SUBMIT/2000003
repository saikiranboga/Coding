#include<iostream>
#include<cstdio>
using namespace std;

int n,m,q,r1,r2;
int a[1024][1024],b[1024][1024],ans;

int main()
{
    int i,j,res,g,br;
    scanf("%d%d",&n,&m);
    for(i=1;i<=n;i++)
     for(j=1;j<=m;j++){
      scanf("%d",&a[i][j]);if(a[i][j]==0)b[i][j]=b[i][j-1]+1;}
    scanf("%d",&q);
    for(;q;q--)
    {
        scanf("%d%d",&r1,&r2);
        for(i=r1;i<=r2;i++)
         for(j=1;j<=m;j++)
         {
                res=b[i][j];
                for(g=i;g>=r1;g--)
                {
                    res=min(res,b[g][j]);
                    if(((i-g)+1)*res>ans)ans=((i-g)+1)*res;
                }
         }
        printf("%d\n",ans);
        ans=0;
    }
}
