#include<iostream>
#include<cstdio>
#include<algorithm>
using namespace std;

int n,m,a[1000001],used[1000001];

int main()
{
    int i,k,ans,res,j;
    scanf("%d%d",&n,&m);
    for(i=1;i<=n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(;m;m--)
    {
        scanf("%d",&i);
        if(i)swap(a[i],a[i+1]);
        else
        {
            ans=0;
            for(i=2;i<=n;i++){res=0;
             for(j=1;j<i;j++)
             {
                    if(used[j]==0&&a[j]<a[i]){res++;used[j]=1;}
             }ans=max(ans,res);}
            printf("%d\n",ans);
            memset(used,0,sizeof(used));
        }
    }
}
