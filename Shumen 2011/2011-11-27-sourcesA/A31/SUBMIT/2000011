#include <cstdio>
#include <algorithm>
using namespace std;

const int Maxn=1<<21;

int n,m;

int niz[Maxn];

struct loga{
    int niz[Maxn];
    void update(int p, int x){
        for (;p<Maxn;p+=p&-p)niz[p]+=x;
    }
    int sum(int x){
        int ret=0;
        for (;x;x^=x&-x)ret+=niz[x];
        return ret;
    }
}L;

int main(void){
    scanf("%d%d", &n, &m);
    for (int i=0;i<n;++i){
        scanf("%d", niz+i);
    }
    int x;
    for (int kk=0;kk<m;++kk){
        scanf("%d", &x); --x;
        if (x==-1){
            L=loga();
            int maxi=0,t;
            for (int i=0;i<n;++i){
                t=L.sum(niz[i]);
                maxi=max(t,maxi);
                L.update(niz[i],-t+1);
            }
            printf("%d\n", maxi);
        }
        else swap(niz[x],niz[x+1]);
    }
    return 0;
}
