/*
TASK: submarines
LANG: C++
*/
#include <stdio.h>
#include <iostream>
#include <string.h>

using namespace std;

int n, m;
int d[1024];

void read()
{
    scanf("%d %d", &n, &m);
    for(int i = 1; i <= n; i ++)
        scanf("%d", &d[i]);
}

void query()
{
    int cnt[1024];
    memset(cnt, 0, sizeof(cnt));
    for(int i = 1; i <= n; i ++)
        for(int j = i + 1; j <= n; j ++)
            if(d[i] < d[j])
            {
                cnt[j] ++;
                break;
            }
    int x = 0;
    for(int i = 1; i <= n; i ++)
        if(x < cnt[i])
            x = cnt[i];
    printf("%d\n", x);
}

void solve()
{
    int cmd;
    for(int i = 0; i < m; i ++)
    {
        scanf("%d", &cmd);
        if(cmd != 0) swap(d[cmd], d[cmd + 1]);
        else query();
    }
}

int main()
{
    read();
    solve();

    return 0;
}
