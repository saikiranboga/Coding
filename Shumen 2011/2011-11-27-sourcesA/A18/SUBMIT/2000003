#include<cstdio>
#include<iostream>
#include<cstring>
using namespace std;

int dpts[1000010], n, q, ans[1000010];

void read() {
    
    int i;
    scanf("%d%d", &n, &q);
    
    for( i=0; i<n; ++i ) 
        scanf("%d", &dpts[i]);
        
}

void solve () {
    
    int i, j;
    for( i=0; i<q; ++i ) {
        
        int t;
        scanf("%d", &t);
        if( t ) {
            
            swap( dpts[t-1], dpts[t] );
            continue;
        }
        
        int cc=1, mc=0;
        
        for( j=0; j<n; ++j ) 
            
            for( int k=j+1; k<n; k++ )
                if( dpts[k]>dpts[j] ) {
                    
                    ans[k]++;
                    if( mc < ans[k] ) mc=ans[k];
                    break;
                }
            memset( ans, 0, sizeof(ans));
        printf("%d\n", mc);
    }
}
        
        

main () {
    
    read();
    solve();
    return 0;
}