#include <iostream>
#include <cstdio>
#include <cmath>
using namespace std;

int N, K, x[1000], y[1000];
double a[1000];


int main()
{ 
  scanf("%d %d", &N, &K);
  
  for( int i = 0; i < N; i++ )
  scanf("%d %d", &x[i], &y[i]);
  
  double max1, S = 0.0;
  int x1, y1;
  for( int i = 0; i < N; i++ )
  { 
    max1 = 0.0;
    for( int j = 0; j < N; j++ )
    { //cout << x[i] << " -> " << x[j] << 
      double g = sqrt( (abs(x[i]-x[j]))*(abs(x[i]-x[j])) + (abs(y[i]-y[j]))*(abs(y[i]-y[j])) );
      if( g > max1 )  { max1 = g; x1 = i+1; y1 = j+1; }
     }
    a[i] = max1;
    //S += max;
   }
  
  /*double k = S/N;
  printf("%.6lf\n", k);
  printf("%d %d\n", x1, y1);
  cout << "yes" << endl;*/
  
  
  sort(a, a+N);
  
  for( int i = 0; i < K; i++ )
  { 
    printf("%.6lf %.6lf\n", a[i]-2, a[i]);
   }
  
  
  //scanf("%d", &N);
  return 0;
}
/*
7 2
1 2
1 4
2 5
3 2
4 4
5 6
6 5
*/
