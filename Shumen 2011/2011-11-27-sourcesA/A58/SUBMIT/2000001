#include<cstdio>
using namespace std;
int n,m,a[1010][1010],ok[1010][1010];
int main()
{
 int i,i2,i3,i4,i5,i6,l;
 scanf("%d%d",&n,&m);
 for(i=1;i<=n;i++)
  for(i2=1;i2<=m;i2++)scanf("%d",&a[i][i2]);
 for(i=1;i<=n;i++)
  for(i2=1;i2<=m;i2++)
   if(a[i][i2]==0)
    for(i3=i;i3<=n;i3++)
     for(i4=i2;i4<=m;i4++)
      if(a[i3][i4]==0)
      {
       l=0;
       for(i5=i;i5<=i3;i5++)
       {
        if(l)break;
        for(i6=i2;i6<=i4;i6++)
         if(a[i5][i6]){l=1;break;}
       }
       if(l==0)
        if((i3-i+1)*(i4-i2+1)>ok[i][i3])ok[i][i3]=(i3-i+1)*(i4-i2+1);
      }
 scanf("%d",&n);
 for(i=0;i<n;i++)
 {
  scanf("%d%d",&i2,&i3);
  printf("%d\n",ok[i2][i3]);
 }
 return 0;
}
