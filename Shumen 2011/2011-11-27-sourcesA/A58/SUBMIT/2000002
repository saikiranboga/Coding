#include<cstdio>
#include<algorithm>
using namespace std;
struct elem
{
 int a,b,s;
};
int n,m,a[1010][1010],b[1010][1010],ok[1010][1010];
int main()
{
 int i,i2,i3,i4,i5,i6,mi;
 scanf("%d%d",&n,&m);
 for(i=1;i<=m;i++)a[0][i]=a[n+1][i]=1;
 for(i=1;i<=n;i++)
 {
  a[i][0]=a[i][m+1]=1;
  for(i2=1;i2<=m;i2++)
  {
   scanf("%d",&a[i][i2]);
   if(a[i][i2])
   {
    b[i][i2]=i;
   }
   else
   {
    b[i][i2]=b[i-1][i2];
   }
  }
 }
 
 /*
 for(i=1;i<=n;i++)
 {
  for(i2=1;i2<=m;i2++)printf("%d ",b[i][i2]);
  printf("\n");
 }
 */
 
 for(i=1;i<=n;i++)
  for(i2=i;i2<=n;i2++)
  {
   ok[i][i2]=ok[i][i2-1];
   for(i3=1;i3<=m;i3++)
    if(a[i2][i3]==0)
    {
     mi=i2-max(b[i2][i3],i-1);
     for(i4=i3;i4>0;i4--)
     {
      if(a[i2][i4])break;
      if(i2-max(b[i2][i4],i-1)<mi)mi=i2-max(b[i2][i4],i-1);
      if((i3-i4+1)*mi>ok[i][i2])
      {
       //printf("%d %d %d %d       %d -> ",i,i2,i3,i4,ok[i][i2]);
       ok[i][i2]=(i3-i4+1)*mi;
       //printf("%d\n\n\n",ok[i][i2]);
      }
     }
    }
  }
 
 
 
 
 scanf("%d",&n);
 for(i=0;i<n;i++)
 {
  scanf("%d%d",&i2,&i3);
  printf("%d\n",ok[i2][i3]);
 }
 return 0;
}
