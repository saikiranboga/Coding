#include <iostream>
#include <fstream>
using namespace std;

const int N=320,inf=1<<30;
int rez[N][N],a[N][N][N],lim[N][N],val[N][N],n,m,D=-1,p;
char PRINT[8000005],s[2011];

ifstream in("date.in");

inline int min(int a,int b)
{
    return a<b ? a : b;
}

inline int max(int a,int b)
{
    return a>b ? a : b;
}

inline int arie(int x,int z,int y,int t)
{
    return (z-x+1)*(t-y);
}

void get(int& x)
{
    while (s[p]<'0' || '9'<s[p])
        p++;
    x=s[p++]-'0';
}

void read()
{
    cin>>n>>m>>ws;
    for (int i=1;i<=n;i++)
    {
        cin.getline(s,2*m+10);
        p=0;
        for (int j=1;j<=m;j++)
        {
            get(val[i][j]);
            val[i][j]+=val[i][j-1];
        }
    }
}

int bs(int v[],int i,int x)
{
    for (int step=1<<9;step;step>>=1)
        if (i+step<=m && v[i+step]<=x)
            i+=step;
    return i;
}

void print(int x)
{
    if (x<10)
    {
        PRINT[++D]=x+'0';
        return;
    }
    print(x/10);
    PRINT[++D]=x%10+'0';
}

int main()
{
    int i,j,p,q;
    read();
    for (i=0;i<=n;i++)
        for (j=0;j<=n;j++)
            for (p=0;p<=m;p++)
                a[i][j][p]=inf;
    for (i=1;i<=n;i++)
        for (j=0;j<=m;j++)
            lim[i][j]=bs(val[i],j,val[i][j]);
    for (i=n;i;i--)
        for (j=i;j<=n;j++)
            for (p=0;p<=m;p++)
                a[i][j][p]=min(a[i][j-1][p],lim[j][p]);
    for (i=n;i;i--)
        for (j=i;j<=n;j++)
        {
            rez[i][j]=max(rez[i+1][j],rez[i][j-1]);
            for (p=0;p<=m;p++)
                rez[i][j]=max(rez[i][j],arie(i,j,p,a[i][j][p]));
        }
    cin>>q;
    while (q--)
    {
        cin>>i>>j;
        print(rez[i][j]);
        PRINT[++D]='\n';
    }
    cout<<PRINT;
    return 0;
}

