#include<iostream>
using namespace std;
int m,n,q,x,y;
bool t[1<<10][1<<10],acc[1<<10];
int main()
{
    cin>>n>>m;
    for(int i=0;i<n;i++) for(int j=0;j<m;j++) cin>>t[i][j];
    cin>>q;
    for(int w=0;w<q;w++)
    {
        cin>>x>>y;
        x--; y--;
        for(int j=0;j<m;j++)
        {
            acc[j]=0;
            bool e=0;
            for(int i=x;i<=y;i++) if(t[i][j]) {e=1; break;}
            if(!e) acc[j]=1;
        }

        //for(int i=0;i<m;i++) cout<<acc[i]<<" "; cout<<endl;

        int curr, maxbr=curr=0;
        for(int j=0;j<m;j++)
        {
            if(acc[j]) curr++;
            else
            {
                maxbr=max(maxbr, curr);
                curr=0;
            }
        }
        maxbr=max(curr, maxbr);
        maxbr*=(y-x+1);
        cout<<maxbr<<endl;
    }
    return 0;
}
