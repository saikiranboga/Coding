#include<iostream>
#include<algorithm>
using namespace std;
int n,k,num;
struct point
{
    int x,y;
} a[1<<10];
bool cmp(point a, point b)
{
    if(a.x==b.x) return(a.y<b.y);
    return(a.x<b.x);
}
int main()
{
    cin>>n>>k;
    for(int i=0;i<n;i++) cin>>a[i].x>>a[i].y;
    sort(a,a+n,cmp);
    for(int i=0;i<k;i++)
    {
        num=(n-i-1);
        cout<<((double)(a[i].x+a[num].x)/2.0)<<" "<<((double)(a[i].y+a[num].y)/2.0)<<endl;
    }
    return 0;
}
