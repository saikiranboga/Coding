#include<iostream>
using namespace std;
bool t[1<<10][1<<10];
int n,m,q,x,y,col[1<<10],ans;

int solve(int x, int y)
{
    int ans=0;
    for(int cn=0;cn<m;cn++)
    {
        col[cn]=0;
        for(int i=x;i<=y;i++) if(t[i][cn]){col[cn]=1; break;}
        if(col[cn]) col[cn]=0;
        else if(cn) col[cn]=col[cn-1]+1;
        else col[cn]=1;
        ans=max(ans,col[cn]);
    }
    return ans*(y-x+1);
}

int main()
{
    cin>>n>>m;
    for(int i=0;i<n;i++) for(int j=0;j<m;j++) cin>>t[i][j];
    cin>>q;
    for(int i=0;i<q;i++)
    {
        cin>>x>>y;
        ans=solve(x-1,y-1);
        cout<<ans<<endl;
    }
    return 0;
}
