#include<iostream>
#include<cstring>
#include<algorithm>
#include<vector>
#include<cmath>
#include<string>
#include<queue>
using namespace std;
int n,m,q;
bool a[1024][1024];
void read()
{
    int i,j;
    cin>>n>>m;
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=m;j++)
        {
            cin>>a[i][j];
        }
    }
}
int BFS(int i,int j,int y)
{
  //  cout<<"BFS"<<" "<<i<<" "<<j<<endl;
    int c,v,br=0,tm=m;
    for(c=i;c<=y;c++)
    {
        for(v=j;v<=tm;v++)
        {
            if(!a[c][v])  br++;
            else
            { 
                tm=v;
                c++;
            }
        }
    }
   // cout<<br<<endl;
    return br;
}
void solve(int x,int y)
{
    int i,j,maxi=0,s;
    for(i=x;i<=y;i++)
    {
        for(j=0;j<m;j++)
        {
            if(a[i][j]==0)
            {
                s=BFS(i,j,y);
                if(maxi<s) maxi=s;
                
            }
        }
    }
    cout<<maxi<<endl;
    
}
int main()
{
    read();
    int i,x,y;
    cin>>q;
    for(i=0;i<q;i++)
    {
        cin>>x>>y;
        solve(x,y);
    }
    return 0;
}