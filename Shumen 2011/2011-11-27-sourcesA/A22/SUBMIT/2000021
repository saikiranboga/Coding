#include<iostream>
#include<cstring>
#include<algorithm>
#include<vector>
#include<cmath>
#include<string>
#include<queue>
using namespace std;
int n,m,q;
bool a[1024][1024];
void read()
{
    int i,j;
    cin>>n>>m;
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=m;j++)
        {
            cin>>a[i][j];
        }
    }
}
int ED(int i,int j,int y)
{
    int c,v,br=0,tm=m,maxia=0;
    for(c=i;c<=y;c++)
    {
        for(v=j;v<=tm;v++)
        {
            if(a[c][v]) 
            { 
                br=0;
                tm=v;
                break;
                c=y+1;
            }
            br++;
        }
        maxia+=br;
    }
    return br;
}
void solve(int x,int y)
{
    int i,j,l=0,maxi=0,s;
    for(i=x;i<=y;i++)
    {
        for(j=0;j<m;j++)
        {
            if(a[i][j]==0)
            {
                s=ED(i,j,y);
                if(maxi<s) maxi=s;
                l=1;
            }
          
        }
    }
    cout<<maxi<<endl;
    
}
int main()
{
    read();
    int i,x,y;
    cin>>q;
    for(i=0;i<q;i++)
    {
        cin>>x>>y;
        solve(x,y);
    }
    return 0;
}