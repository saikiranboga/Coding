#include<iostream>
#include<cstring>
#include<algorithm>
#include<vector>
#include<cmath>
#include<string>
#include<queue>
using namespace std;
int n,k;
struct sheep
{
    int x,y;
};
sheep cheat[1024];
int maxx,maxy;
void read()
{
    int i,a,b;
    cin>>n>>k;
    for(i=0;i<n;i++)
    {
        cin>>a>>b;
        if(a>maxx) maxx=a;
        if(b>maxy) maxy=b;
        cheat[i].x=a;
        cheat[i].y=b;
    }
    if(maxx%2) cout<<maxx/2<<".500000"<<" ";
    else cout<<maxx/2<<".000000"<<" ";
    if(maxy%2) cout<<maxy/2<<".500000"<<endl;
    else cout<<maxy/2<<".000000"<<endl;
    for(i=1;i<k;i++)
    {
        cout<<cheat[(i/2+1)].x<<".000000"<<" "<<cheat[(i/2-1)].y<<".000000"<<endl;
    }
    
}
int main()
{
    read();
    return 0;
}