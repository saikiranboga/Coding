#include <iostream>
#include <cstring>
#include <cstdio>


using namespace std;

pair < double, double > x[1 << 20];
int N, M;

void scan(){
  scanf ( "%d%d", &N, &M );

  for ( int i = 0; i < N; ++i ){
    x[i].first = i * 5000000LL;
    cin >> x[i].second;
  }
}

int br[1 << 20];

double dist ( pair < double, double > t1, pair < double, double > t2 ){
  return ( t1.first - t2.first ) * ( t1.first - t2.first ) + ( t1.second - t2.second ) * ( t1.second - t2.second ) ;
}

void print(){
  for ( int i = 0; i < N; ++i )
    br[i] = 0;
  
  for ( int i = 0; i < N; ++i ){
    int t = -1;
    
    for ( int j = i + 1; j < N; ++j )
      if ( x[i].second < x[j].second )
	if ( t == -1 ) t = j;
//	else
//	  if ( dist ( x[t], x[i] ) > dist ( x[j], x[i] ) ) t = j;
    ++br[t];
    
  }
  
  int mx = 0;
  
  for ( int i = 0; i < N; ++i )
      mx = max ( mx, br[i] );
  
  cout << mx << endl;
}

void change ( int i ){
  swap ( x[i].second, x[i + 1].second );
}
void solve(){
  
  
  for ( int i = 0; i < M; ++i ){
    int x;
    
    scanf ( "%d", &x );
    --x;
    
    if ( x == -1 )
      print();
    else
      change(x);
  }
}

int main(){
  scan();
  solve();
}