#include<iostream>
#include<cstdio>
#include<cmath>
#include<algorithm>
#include<queue>
#include<stack>
#include<vector>
#include<map>
#include<string>
#include<cstring>
using namespace std;
int N,M,a[100000];
int dp[10000000];
void read ()
{
        scanf("%d%d",&N,&M);
        int i;
        for(i=1;i<=N;i++){
        scanf("%d",&a[i]);
        }
}
vector<int> v;
int ask ( int x)
{
    int sz=v.size();
    int l=0,r=sz-1;
    int mid,gg;
    while(l<=r)
    {
         mid=(l+r)/2;
         if(v[mid]<x){gg=mid;r=mid-1;}
         else l=mid+1;
    }
    return sz-gg;
}
int ans ()
{
    dp[1]=1;
    v.clear();
    v.push_back(a[1]);
    int ret=0,prev=1;
    int i,tmp;
    for(i=2;i<=N;i++)
    {
        if(a[i]<a[i-1])
        {
            v.push_back(a[i]);
        }
        else
        {
            tmp=ask(a[i]);
            for(int j=1;j<=tmp;j++)v.pop_back();
            v.push_back(a[i]);
            if(tmp>ret)ret=tmp;
            //cout<<i<<' '<<ret<<endl;
        }
    }
    return ret;
}
void solve()
{
    int i,b;
    for(i=1;i<=M;i++)
    {
        scanf("%d",&b);
        if(b==0)printf("%d\n",ans());
        else
        {
            swap(a[b],a[b+1]);
        }
    }
}
int main ()
{
    read();
    solve();
    return 0;
}
